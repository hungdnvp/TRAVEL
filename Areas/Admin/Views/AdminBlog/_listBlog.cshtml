          @{
               foreach (Blog item in Model)
               {
                    <tr class="table-dark">
                         <td><i class="fab fa-bootstrap fa-lg text-primary me-3"></i> <strong>@item.Ten</strong></td>
                         <td>@item.NgayDang.ToString().Split(' ')[0]</td>
                         <td>
                              <ul class="list-unstyled users-list m-0 avatar-group d-flex align-items-center">
                                   <li data-bs-toggle="tooltip"
                                       data-popup="tooltip-custom"
                                       data-bs-placement="top"
                                       class="avatar avatar-xs pull-up"
                                       title=@item.TacGia>
                                        <img src="/assets/@item.TaiKhoan.ChiTietTKs.FirstOrDefault().LinkAvatar" alt="Avatar" class="rounded-circle" />
                                   </li>
                              </ul>
                         </td>
                         <td><a href="/Admin/AdminBlog/CategoryFilter?category=@item.Category" id="categoryFilter"><span class="badge bg-label-success me-1" id="spancategory">@item.Category</span></a></td>
                         <td>
                              <div class="dropdown">
                                   <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                                        <i class="bx bx-dots-vertical-rounded"></i>
                                   </button>
                                   <div class="dropdown-menu">
                                        <a class="dropdown-item" href="/Admin/AdminBlog/Edit?idBlog=@item.Blog_ID"><i class="bx bx-edit-alt me-1"></i> Edit</a>
                                        <a class="dropdown-item" href="/Admin/AdminBlog/Delete?idBlog=@item.Blog_ID"><i class="bx bx-trash me-1"></i> Delete</a>
                                   </div>
                              </div>
                         </td>
                    </tr>
               }
